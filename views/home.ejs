                <ul
                  class="rslides callbacks callbacks1"
                  id="slider3"
                  style="height: 100px"
                ></ul>
              </div>
              <div class="clearfix"></div>
              <script>
                // You can also use "$(window).load(function() {"
                $(function () {
                  // Slideshow 4
                  $("#slider3").responsiveSlides({
                    auto: true,
                    pager: true,
                    nav: false,
                    speed: 500,
                    namespace: "callbacks",
                    before: function () {
                      $(".events").append("<li>before event fired.</li>");
                    },
                    after: function () {
                      $(".events").append("<li>after event fired.</li>");
                    },
                  });
                });
              </script>
              <!--banner Slider starts Here-->
            </div>
          </div>
        </div>
