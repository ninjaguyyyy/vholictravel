                <ul class="w3_address">
                  <li>
                    <i class="fa fa-map-marker" aria-hidden="true"></i>148 Trần
                    Quang Khải, Phường Tân Định, Quận 1, Thành Phố Hồ Chí Minh.
                  </li>
                  <li>
                    <i class="fa fa-envelope-o" aria-hidden="true"></i
                    ><a href="mailto:vholictravel148@gmail.com"
                      >vholictravel148@gmail.com</a
                    >
                  </li>
                  <li>
                    <i class="fa fa-mobile" aria-hidden="true"></i>036 557 6259
                  </li>
                </ul>
              </div>
              <div class="contact-form">
                <h4>Liên hệ hỗ trợ</h4>
                <form action="#" method="post">
                  <div class="styled-input agile-styled-input-top">
                    <input type="text" name="Name" required="" />
                    <label>Họ tên</label>
                    <span></span>
                  </div>
                  <div class="styled-input">
                    <input type="text" name="Email" required="" />
                    <label>Email</label>
                    <span></span>
                  </div>
                  <div class="styled-input">
                    <input type="text" name="Subject" required="" />
                    <label>Tiêu đề</label>
                    <span></span>
                  </div>
                  <div class="styled-input">
                    <textarea name="Message" required=""></textarea>
                    <label>Nội dung cần tư vấn</label>
                    <span></span>
                  </div>
                  <input type="submit" value="Gửi" />
                </form>
              </div>
            </div>
