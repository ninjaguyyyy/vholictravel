            <div id="owl-demo" class="owl-carousel owl-theme">
              <% tours.forEach(function(tour, i){ %> <% let classBg = `bg_${i
              +1}` %>
              <div class="item">
                <div class="special-info <%= classBg %>">
                  <div class="special-info-top">
                    <div class="special-star">
                      <i class="fa fa-star" aria-hidden="true"></i>
                      <i class="fa fa-star" aria-hidden="true"></i>
                      <i class="fa fa-star" aria-hidden="true"></i>
                      <i class="fa fa-star" aria-hidden="true"></i>
                    </div>
                    <h4>
                      <a href="/detail/<%= tour.id %>" style="color: #fff"
                        ><%= tour.name %></a
                      >
                    </h4>
                  </div>
                  <div class="special-info-bottom">
                    <ul>
                      <li>65 <span>Hotels</span></li>
                      <li>24 <span>Cities</span></li>
                      <li>45 <span>Tours</span></li>
                    </ul>
                    <p style="font-size: 20px; background-color: #00000085">
                      <%= tour.price %>k/người
                    </p>
                  </div>
                </div>
              </div>
              <% }) %>
            </div>
          </div>
        </div>
